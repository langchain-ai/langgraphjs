{
  "name": "vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "yarn turbo:command build:internal --filter=vite",
    "dev": "turbo dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "tsx watch --env-file=.env --clear-screen=false src/server.mts",
    "build:internal": "tsc -b && vite build",
    "format": "prettier --write src",
    "lint": "prettier --check src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "@langchain/core": "^1.0.0-alpha",
    "@langchain/langgraph": "workspace:*",
    "@langchain/langgraph-sdk": "workspace:*",
    "@langchain/openai": "^1.0.0-alpha",
    "hono": "^4.8.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.4.1",
    "prettier": "^2.8.3",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "vite": "^6.0.0"
  }
}

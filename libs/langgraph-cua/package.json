{
  "name": "@langchain/langgraph-cua",
  "version": "1.0.0",
  "description": "An implementation of a computer use agent (CUA) using LangGraph",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/langchain-ai/langgraphjs/blob/main/libs/langgraph-cua/README.md",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/langchain-ai/langgraphjs.git",
    "directory": "libs/langgraph-cua"
  },
  "scripts": {
    "build": "yarn turbo build:internal",
    "build:internal": "yarn workspace @langchain/build compile @langchain/langgraph-cua",
    "clean": "rm -rf dist/ dist-cjs/ .turbo/",
    "lint:eslint": "NODE_OPTIONS=--max-old-space-size=4096 eslint --cache --ext .ts,.js src/",
    "lint:dpdm": "dpdm --exit-code circular:1 --no-warning --no-tree src/*.ts src/**/*.ts",
    "lint": "yarn lint:eslint && yarn lint:dpdm",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:dpdm",
    "prepublish": "yarn build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:int": "vitest run --mode int",
    "format": "prettier --config .prettierrc --write \"src\" \"examples\"",
    "format:check": "prettier --config .prettierrc --check \"src\" \"examples\""
  },
  "author": "LangChain",
  "license": "MIT",
  "dependencies": {
    "scrapybara": "^2.4.4",
    "zod": "^3.25.76 || ^4"
  },
  "peerDependencies": {
    "@langchain/core": "^1.0.1",
    "@langchain/langgraph": "^0.2.57 || ^0.3.0 || ^0.4.0 || ^1.0.0-alpha.5",
    "@langchain/openai": "^1.0.0"
  },
  "devDependencies": {
    "@langchain/langgraph": "workspace:*",
    "@langchain/openai": "^1.0.0",
    "@langchain/scripts": "^0.1.3",
    "@tsconfig/recommended": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "dotenv": "^16.3.1",
    "dpdm": "^3.12.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "openai": "^4.87.3",
    "prettier": "^2.8.3",
    "tsx": "^4.19.3",
    "turbo": "^2.5.4",
    "typescript": "^4.9.5 || ^5.4.5",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "input": "./src/index.ts",
      "typedoc": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./utils": {
      "input": "./src/utils.ts",
      "typedoc": "./src/utils.ts",
      "import": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.js"
      },
      "require": {
        "types": "./dist/utils.d.cts",
        "default": "./dist/utils.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/"
  ],
  "resolutions": {
    "typescript": "4.9.5"
  }
}

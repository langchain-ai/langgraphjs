{
  "name": "@langchain/langgraph-ui",
  "version": "1.1.11",
  "type": "module",
  "engines": {
    "node": "^18.19.0 || >=20.16.0"
  },
  "license": "MIT",
  "main": "./dist/index.mjs",
  "bin": {
    "langgraphjs-ui": "./dist/cli.mjs"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/langchain-ai/langgraphjs.git",
    "directory": "libs/langgraph-ui"
  },
  "scripts": {
    "clean": "rm -rf dist/ .turbo/",
    "build": "pnpm turbo build:internal --filter=@langchain/langgraph-ui",
    "build:internal": "pnpm clean && node scripts/build.mjs",
    "prepublish": "pnpm build",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "commander": "^14.0.2",
    "esbuild": "^0.25.0",
    "esbuild-plugin-tailwindcss": "^2.0.1",
    "zod": "^3.25.76 || ^4"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "prettier": "^3.8.1",
    "typescript": "^4.9.5 || ^5.4.5",
    "vitest": "^4.0.18"
  }
}

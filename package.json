{
  "name": "langgraph",
  "private": true,
  "description": "LangGraph",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:langchain-ai/langgraphjs.git"
  },
  "workspaces": [
    "langgraph"
  ],
  "scripts": {
    "build": "turbo run build --concurrency 1",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "clean": "turbo run clean",
    "test": "turbo run test",
    "test:int": "turbo run test:int",
    "format": "turbo run format",
    "format:check": "turbo run format:check"
  },
  "author": "LangChain",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.0.17",
    "@langchain/core": "^0.1.14",
    "@langchain/openai": "^0.0.12",
    "langchain": "^0.1.3",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@swc/core": "^1.3.90",
    "@swc/jest": "^0.2.29",
    "@tsconfig/recommended": "^1.0.3",
    "dpdm": "^3.12.0",
    "commander": "^11.1.0",
    "jest": "^29.5.0",
    "@types/semver": "^7",
    "jest-environment-node": "^29.6.4",
    "release-it": "^15.10.1",
    "semver": "^7.5.4",
    "tsx": "^4.7.0",
    "typescript": "<5.2.0",
    "turbo": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "index.cjs",
    "index.js",
    "index.d.ts"
  ]
}
